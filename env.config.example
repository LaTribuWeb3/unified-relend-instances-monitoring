# Unified Relend Instances Monitoring Dashboard Environment Variables
# Copy this file to .env and update the values as needed

# API Endpoints for different environments
REACT_APP_L1_API_ENDPOINT=http://localhost:3001/api/l1
REACT_APP_L2_API_ENDPOINT=http://localhost:3002/api/l2

# Optional: Add additional configuration
REACT_APP_REFRESH_INTERVAL=30000
REACT_APP_ENABLE_AUTO_REFRESH=true

# Optional: Environment-specific settings
REACT_APP_ENVIRONMENT=development
REACT_APP_DEBUG_MODE=true 